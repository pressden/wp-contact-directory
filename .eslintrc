{
	"extends": "@10up/eslint-config",
	"globals": {
		"wp": true
	},
	"rules": {
		"linebreak-style": [
			2,
			"unix"
		],
		"quotes": [
			2,
			"single"
		],
		"semi": [
			2,
			"always"
		],
		"camelcase": [
			0
		],
		"no-unused-vars": [
			0
		],
		"no-console": [
			0
		],
		"require-jsdoc": [
			0
		],
		"yoda": [
			0
		]
	}
}
